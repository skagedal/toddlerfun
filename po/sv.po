#: ../toddlerfun.desktop.in.h:1 ../src/main.c:798
msgid "Toddler Fun"
msgstr "Skoj för Småbarn"

#: ../toddlerfun.desktop.in.h:2
msgid "A toy app for the smallest children"
msgstr "Ett leksaksprogram för de minsta barnen"

#: ../src/main.c:64
msgid ""
"Welcome to Toddler Fun! Move the mouse around to draw. Press Escape to exit."
msgstr "Välkommen till Skoj för Småbarn! Rita med musen. Tryck Escape för att avsluta."

#: ../src/main.c:65
msgid ""
"Press mouse buttons to add funny images. Press keys on the keyboard to add "
"letters."
msgstr "Tryck på musknapparna för att lägga in roliga bilder. Skriv bokstäver på tangentbordet."

#: ../src/main.c:66
msgid "Press Return to save the current image to a file."
msgstr "Tryck Return för att spara bilden till en fil."

#: ../src/main.c:67
msgid "Use the mouse scroll wheel or the Up/Down keys to change mirror effect."
msgstr "Använd musens scrollhjul eller Upp/Ner-tangenterna för att ändra spegeleffekt."

#: ../src/main.c:68
msgid "Press Space to clear drawing."
msgstr "Tryck mellanslag för att rensa teckningen."

#: ../src/main.c:109
#, c-format
msgid "Sound file '%s' does not exist\n"
msgstr "Ljudfilen '%s' finns inte\n"

#: ../src/main.c:384
#, c-format
msgid "Failed to create directory '%s'\n"
msgstr "Misslyckades med att skapa katalogen '%s'\n"

#: ../src/main.c:393
#, c-format
msgid "Failed to create file '%s'\n"
msgstr "Misslyckades med att skapa filen '%s'\n"

#: ../src/main.c:741
#, c-format
msgid "Can't load %s: %s\n"
msgstr "Kunde inte ladda %s: %s\n"

#: ../src/main.c:780
msgid "Don't run in fullscreen mode"
msgstr "Kör inte i fullskärmsläge"

#: ../src/main.c:782
msgid "Don't play music"
msgstr "Spela inte musik"

#: ../src/main.c:784
msgid "Don't play sound effects"
msgstr "Spela inte ljudeffekter"

#: ../src/main.c:802
msgid "A drawing toy for toddlers."
msgstr "En ritleksak för småbarn."

#: ../src/main.c:808
#, c-format
msgid "option parsing failed: %s\n"
msgstr "misslyckades läsa växel: %s\n"

#: ../src/theme.c:96
#, c-format
msgid "Theme parser error\n"
msgstr "Kunde inte läsa temat\n"

#: ../src/theme.c:133 ../src/theme.c:140
#, c-format
msgid "Parse error on theme file %s: %s\n"
msgstr "Kunde inte läsa temafilen %s: %s\n"
